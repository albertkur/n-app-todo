{
    "name": "n-app-todo",
    "version": "0.0.1",
    "description": "n app todo app",
    "type": "commonjs",
    "main": "src/server/server.js",
    "packageManager": "yarn@4.5.0",
    "scripts": {
        "ts-lint": "eslint",
        "ts-compile-server": "tsc --build tsconfig.server.json",
        "ts-compile-server-watch": "tsc --build tsconfig.server.json -w",
        "ts-compile-client": "tsc --build tsconfig.client.json",
        "ts-build": "yarn ts-compile-server && yarn ts-compile-client && yarn ts-lint",
        "webpack-build": "webpack",
        "nodemon": "node --watch-path=src/server/ --inspect=9210 --enable-source-maps ./src/server/server.js",
        "dev": "yarn ts-build && concurrently \"yarn:nodemon\"",
        "clean-src": "find . -type f -and -not -path \"*/node_modules/*\" -and -not -path \"*/3rd-party/*\" -and -not -name \"eslint.config.js\" -and \\( -name \"*.js\" -or -name \"*.d.ts\" -or -name \"*.js.map\" -or -name \"*.ts.map\" -or -name \"*.tsbuildinfo\" \\) -delete && rm -rf dist && rm -rf src/client/dist/",
        "build": "yarn ts-build && env=prod yarn webpack-build",
        "test": "yarn ts-build && node --test --enable-source-maps './test/**/*.test.js'",
        "start": "node --enable-source-maps ./src/server/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shreymahendru/n-app-todo.git"
    },
    "keywords": [
        "n-app",
        "todo",
        "web",
        "app"
    ],
    "author": "Shrey Mahendru",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/shreymahendru/n-app-todo/issues"
    },
    "homepage": "https://github.com/SofDevInc/n-app-todo#readme",
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@eslint/js": "^9.39.0",
        "@stylistic/eslint-plugin": "^5.5.0",
        "@types/hash-sum": "^1.0.2",
        "@types/jquery": "^3.5.8",
        "@types/loader-utils": "^2.0.0",
        "@types/minimatch": "^6.0.0",
        "@types/mocha": "^8.2.2",
        "@types/node": "^24.0.0",
        "@typescript-eslint/parser": "^8.23.0",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
